{
    "compilerOptions": {
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "dist",
        "sourceMap": false,
        "strict": true,
        "target": "es2015"
    },
    "exclude": [
        "node_modules"
    ]
}